\n      gridHeight: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    openWindow(windowId) {\r\n      const payload = {\r\n        windowState: \"open\",\r\n        windowID: windowId,\r\n      };\r\n      this.$store.commit(\"setWindowState\", payload);\r\n    },\r\n    openGithub() {\r\n      window.open(\"https://github.com/DonChiaQE\");\r\n    },\r\n  },\r\n  mounted() {\r\n    let gridH = this.$store.getters.getFullscreenWindowHeight;\r\n    this.gridHeight = gridH.substring(0, gridH.length - 2) - 60 + \"px\";\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAoCA;EACAA,IAAA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,OAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC,UAAA;MACAC,UAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAAC,QAAA;MACA,MAAAC,OAAA;QACAC,WAAA;QACAC,QAAA,EAAAH;MACA;MACA,KAAAN,MAAA,CAAAU,MAAA,mBAAAH,OAAA;IACA;IACAI,WAAA;MACAC,MAAA,CAAAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,IAAAC,KAAA,QAAAf,MAAA,CAAAC,OAAA,CAAAe,yBAAA;IACA,KAAAb,UAAA,GAAAY,KAAA,CAAAE,SAAA,IAAAF,KAAA,CAAAG,MAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}