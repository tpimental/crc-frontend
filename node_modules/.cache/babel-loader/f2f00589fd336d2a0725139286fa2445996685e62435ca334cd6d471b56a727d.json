n  border-top: solid rgb(0, 0, 0) 1.5px;\r\n  border-left: solid rgb(0, 0, 0) 1.5px;\r\n  border-bottom: solid rgb(250, 250, 250) 1.5px;\r\n  border-right: solid rgb(250, 250, 250) 1.5px;\r\n}\r\n\r\n.start-menu-depressed {\r\n  box-shadow: none;\r\n  background: repeating-conic-gradient(\r\n      rgb(189, 190, 189) 0% 25%,\r\n      rgb(255, 255, 255) 0% 50%\r\n    )\r\n    50% / 2px 2px;\r\n  border-top: solid rgb(0, 0, 0) 1.5px;\r\n  border-left: solid rgb(0, 0, 0) 1.5px;\r\n  border-bottom: solid rgb(250, 250, 250) 1.5px;\r\n  border-right: solid rgb(250, 250, 250) 1.5px;\r\n}\r\n\r\n.container-border {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.border {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  border: black dotted 1px;\r\n}\r\n\r\n/*-------------------------------------------*\\\r\n      Utilities\r\n  \\*-------------------------------------------*/\r\n\r\n.icon-image {\r\n  width: 15px;\r\n  height: 15px;\r\n  margin-right: 5px;\r\n  margin-top: 0;\r\n  margin-bottom: 0;\r\n}\r\n\r\n.spacer {\r\n  flex-grow: 1;\r\n}\r\n\r\n.time {\r\n  width: 75px;\r\n  margin: 5px;\r\n  height: 25px;\r\n  background: rgb(192, 192, 192);\r\n  border-right: solid rgb(250, 250, 250) 1.5px;\r\n  border-bottom: solid rgb(250, 250, 250) 1.5px;\r\n  border-top: solid rgb(90, 90, 90) 1.5px;\r\n  border-left: solid rgb(90, 90, 90) 1.5px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: row;\r\n  font-size: 0.6em;\r\n}\r\n\r\n@media only screen and (max-width: 700px) {\r\n  .time {\r\n    width: auto !important;\r\n    padding-left: 5px;\r\n    padding-right: 5px;\r\n  }\r\n  .icon-image {\r\n    width: 0px;\r\n    height: 0px;\r\n    margin-right: 0px;\r\n  }\r\n  .navbar-item {\r\n    width: auto;\r\n    padding-top: 0;\r\n  }\r\n\r\n  .navbar-item-depressed {\r\n    width: auto;\r\n    padding-top: 0;\r\n  }\r\n}\r\n\r\n/*-------------------------------------------*\\\r\n      CSS Normalisation \r\n  \\*-------------------------------------------*/\r\n\r\nbutton {\r\n  background: none;\r\n  color: inherit;\r\n  border: none;\r\n  font: inherit;\r\n  outline: inherit;\r\n}\r\n\r\np {\r\n  margin: 0;\r\n  padding: 0;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport moment from \"moment\";\r\nexport default {\r\n  name: \"Navbar\",\r\n  data: function () {\r\n    return {\r\n      activeWindows: this.$store.getters.getActiveWindows,\r\n\r\n      // date time for moment.js\r\n      time: \"\",\r\n      date: \"\",\r\n    };\r\n  },\r\n  beforeMount() {\r\n    setInterval(() => {\r\n      this.time = moment().format(\"hh:mm A\");\r\n    }, 1000);\r\n    setInterval(() => {\r\n      this.date = moment().format(\"ddd DD MMMM\");\r\n    }, 1000);\r\n  },\r\n  methods: {\r\n    openWindow(windowId) {\r\n      const payload = {\r\n        windowState: \"open\",\r\n        windowID: windowId,\r\n      };\r\n      this.$store.commit(\"setWindowState\", payload);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AA8RA,OAAAA,MAAA;AACA;EACAC,IAAA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,aAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC,gBAAA;MAEA;MACAC,IAAA;MACAC,IAAA;IACA;EACA;EACAC,YAAA;IACAC,WAAA;MACA,KAAAH,IAAA,GAAAP,MAAA,GAAAW,MAAA;IACA;IACAD,WAAA;MACA,KAAAF,IAAA,GAAAR,MAAA,GAAAW,MAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAAC,QAAA;MACA,MAAAC,OAAA;QACAC,WAAA;QACAC,QAAA,EAAAH;MACA;MACA,KAAAV,MAAA,CAAAc,MAAA,mBAAAH,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}